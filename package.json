{
  "name": "await-invocation",
  "version": "1.0.0",
  "description": "Provides the ability to await until a function has been invoked. Created to help test asynchronous functions without having to wait for them to finish, namely Temporal.io workflows.",
  "keywords": ["Temporal", "Temporal.io", "await", "promises", "function", "invoke", "invocation"],
  "homepage": "https://github.com/davidleston/await-invocation#readme",
  "bugs": {
    "url": "https://github.com/davidleston/await-invocation/issues"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": {
    "name": "David Leston",
    "url": "https://DavidLeston.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidleston/await-invocation.git"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/await-invocation.umd.cjs",
  "module": "./dist/await-invocation.js",
  "types": "./dist/await-invocations.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "typescript": "4.9.5",
    "vite": "4.1.2",
    "vite-plugin-dts": "2.0.0-beta.1",
    "vitest": "0.28.5"
  }
}